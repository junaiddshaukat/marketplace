{
    "version": 2,
    "builds": [
      {
        "src": "server.ts",
        "use": "@vercel/node",
        "config": { "includeFiles": ["**/*.ts"] }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.ts"
      }
    ],
    "env": {
      "NODE_ENV": "development"
    }
  }
  